<?xml version="1.0"?>

<PropertyList>

    <path>starter_ann.ac</path>

    <!--	Starter Annunciator	-->
    <animation>
        <type>select</type>
        <object-name>starter_ann</object-name>
        <condition>
            <and>
                <or>
                    <property>controls/engines/engine[0]/starter</property>
                    <property>controls/engines/engine[1]/starter</property>
                </or>
                <property>controls/switches/master-bat</property>
            </and>
        </condition>
    </animation>

</PropertyList>


