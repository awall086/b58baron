<?xml version="1.0"?>

<PropertyList>

    <path>lgen_ann.ac</path>

    <!--	R Gen Annunciator	-->
    <animation>
        <type>select</type>
        <object-name>lgen_ann</object-name>
        <condition>
            <and>
                <less-than>
                    <property>/systems/electrical/suppliers/alternator_l</property>
                    <value>21.0</value>
                </less-than>
                <property>controls/switches/master-bat</property>
            </and>
        </condition>
    </animation>

</PropertyList>


