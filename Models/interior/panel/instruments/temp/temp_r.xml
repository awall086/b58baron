<?xml version="1.0"?>

<PropertyList>

 <path>temp.ac</path>

    <effect>    
        <inherits-from>Effects/glass</inherits-from>
        <object-name>cristal</object-name>
    </effect>
    <!-- Right Engine cht temp indicator -->

    <animation>
        <type>rotate</type>
        <object-name>chttemp_needle</object-name>
        <property>engines/engine[1]/cht-degf</property>
        <interpolation>
            <entry>
                <ind>182</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>482</ind>
                <dep>-75</dep>
            </entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- Right Engine egt temp indicator -->

    <animation>
        <type>rotate</type>
        <object-name>egttemp_needle</object-name>
        <property>engines/engine[1]/egt-degf</property>
        <interpolation>
            <entry>
                <ind>68</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>572</ind>
                <dep>70</dep>
            </entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
   </animation>

</PropertyList>


