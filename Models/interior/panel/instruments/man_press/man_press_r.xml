<?xml version="1.0"?>

<PropertyList>

 <path>man_press.ac</path>

    <effect>    
        <inherits-from>Effects/glass</inherits-from>
        <object-name>cristal</object-name>
    </effect>
    
    <!-- Right Engine Manifold pressure indicator -->
    
    <animation>
        <type>rotate</type>
        <object-name>manpress_needle</object-name>
        <property>/engines/engine[1]/mp-osi</property>
        <interpolation>
            <entry>
                <ind>10</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>40</ind>
                <dep>250</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>


