<?xml version="1.0"?>

<PropertyList>

 <path>fuel_flow.ac</path>

    <effect>    
        <inherits-from>Effects/glass</inherits-from>
        <object-name>cristal</object-name>
    </effect>
     <!-- Left Engine fuel flow indicator -->
    <animation>
        <type>rotate</type>
        <object-name>fuelflow_needle</object-name>
        <property>engines/engine[0]/fuel-flow-gph</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>35</ind>
                <dep>263</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>


