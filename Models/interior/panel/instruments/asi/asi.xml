    <!-- Airspeed indicator -->
    <animation>
        <type>rotate</type>
        <object-name>airspeed_needle</object-name>
        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
        <interpolation>
            <entry>
                <ind> 0</ind>
                <dep> 0</dep>
            </entry>
            <entry>
                <ind> 35</ind>
                <dep> 0</dep>
            </entry>
            <entry>
                <ind> 40</ind>
                <dep> 0</dep>
            </entry>
            <entry>
                <ind> 60</ind>
                <dep> 16</dep>
            </entry>
            <entry>
                <ind> 80</ind>
                <dep> 66</dep>
            </entry>
            <entry>
                <ind> 100</ind>
                <dep> 100</dep>
            </entry>
            <entry>
                <ind> 120</ind>
                <dep> 135</dep>
            </entry>
            <entry>
                <ind> 140</ind>
                <dep> 170</dep>
            </entry>
            <entry>
                <ind> 160</ind>
                <dep> 211</dep>
            </entry>
            <entry>
                <ind> 180</ind>
                <dep> 247</dep>
            </entry>
            <entry>
                <ind> 220</ind>
                <dep> 290</dep>
            </entry>
            <entry>
                <ind> 260</ind>
                <dep> 320</dep>
            </entry>
            <entry>
                <ind> 270</ind>
                <dep> 330</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>

    </animation> 
 
</PropertyList>
