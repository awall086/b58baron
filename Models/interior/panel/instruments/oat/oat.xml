<?xml version="1.0"?>

<PropertyList>

    <path>oat.ac</path>


    <animation>
        <type>rotate</type>
        <object-name>needle</object-name>
        <property>environment/temperature-degc</property>
       <interpolation>
            <entry>
                <ind> 0</ind>
                <dep> 0</dep>
            </entry>
            <entry>
                <ind> 20</ind>
                <dep> -120</dep>
            </entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
</PropertyList>
