<?xml version="1.0" ?> 
  <PropertyList> 
    <path>yokeL.ac</path> 
<!--
    <effect>
        <inherits-from>Aircraft/c152/Models/Effects/interior/c152-interior-compartment</inherits-from>
        <object-name>yokeL</object-name>
    </effect>
-->
    
 <animation>
  <type>translate</type>
  <object-name>yokeL</object-name>
  <property>controls/flight/elevator</property>
  <factor>-0.1</factor>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>yokeL</object-name>
  <property>controls/flight/aileron</property>
  <factor>-30.0</factor>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
 </animation>

 <animation>
   <type>select</type>
   <object-name>yokeL</object-name>
   <condition>
     <not>
       <property>sim/model/hide-yoke</property>
     </not>
   </condition>
 </animation>
</PropertyList>
